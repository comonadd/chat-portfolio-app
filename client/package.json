  "scripts": {
    "build:release": "babel-node tools/build release",
    "build:dev": "babel-node tools/build dev",
    "build": "yarn run build:release",
    "clean": "babel-node tools/clean",
    "dev-server": "babel-node tools/run_dev_server",
    "format:scripts": "babel-node tools/format scripts",
    "format:styles": "babel-node tools/format styles",
    "format": "yarn run format:scripts && yarn run format:styles",
    "lint": "yarn run lint:scripts && yarn run lint:styles",
    "lint:styles": "stylelint --config conf/stylelintrc.json src/**/*.scss",
    "lint:scripts": "eslint -c conf/eslintrc.yml tools && tslint -c conf/tslint.json 'src/**/*.ts'",
    "test": "yarn run test:single",
    "test:watch": "babel-node ./node_modules/karma/bin/karma start tools/karma.conf.js",
    "test:single": "babel-node ./node_modules/karma/bin/karma start tools/karma.conf.js --single-run"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
